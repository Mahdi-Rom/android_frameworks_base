<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Description of the Random button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <string name="keyboardview_keycode_random">Zufällige Anordnung neu laden</string>

    <!-- Smart cover -->
    <string name="keyguard_discharging">Entlädt, <xliff:g id="number">%d</xliff:g><xliff:g id="percent">%%</xliff:g></string>
    
    <plurals
        name="missed_calls">
        <item quantity="one">%d Verpasster Anruf</item>
        <item quantity="other">%d Verpasste Anrufe</item>
    </plurals>
    <plurals
        name="unread_messages">
        <item quantity="one">%d Ungelesene Nachricht</item>
        <item quantity="other">%d Ungelese Nachrichten</item>
    </plurals>
    <string name="missed_call">Verpasster Anruf</string>
    <string name="message_from">Nachricht von</string>
    <string name="message_from_person">Nachricht von %1$s</string>
    <!-- Skeleton string format for displaying the date. -->
    <string name="full_wday_month_day_no_year">EEEEMMMMd</string>

    <!-- On the unlock gesture screen, shown at the top of the unlock screen to tell the user what to do. Below this text is the place for the user to draw the gesture. -->
    <string name="lockscreen_gesture_instructions">Geste zum Enstperren zeichnen</string>
    <!-- Shown to confirm that the user entered their lock gesture correctly. -->
    <string name="lockscreen_gesture_correct">Richtig!</string>
    <!-- On the unlock gesture screen, shown when the user enters the wrong lock gesture and must try again. -->
    <string name="lockscreen_gesture_wrong">Versuche es erneut</string>
    <!-- Accessibility description of the gesture unlock. [CHAR_LIMIT=none] -->
    <string name="keyguard_accessibility_gesture_unlock">Gestenentsperrung</string>
    <!-- Accessibility description of the unlock gesture area. [CHAR_LIMIT=none] -->
    <string name="keyguard_accessibility_gesture_area">Gestenbereich</string>
    <!-- Message shown in gesture unlock after some number of unsuccessful attempts -->
    <string name="kg_forgot_gesture_button_text">Geste vergessen</string>
    <!-- Message shown when user enters wrong pattern -->
    <string name="kg_wrong_gesture">Falsche Geste</string>
    <!-- Instructions for using the gesture unlock screen -->
    <string name="kg_gesture_instructions">Zeichne deine Geste</string>
    <string name="kg_too_many_failed_gesture_attempts_dialog_message">
        Du hast deine Entsperrgeste <xliff:g id="number">%d</xliff:g> mal falsch gezeichnet.
        \n\nVersuche es in <xliff:g id="number">%d</xliff:g> Sekunden erneut.
    </string>

    <!-- Description of accessibility direction for targets [CHAR LIMIT=NONE] -->
    <string name="accessibility_target_direction">Wischen um ein Ziel zu aktivieren</string>

</resources>
